nav.o-nav.sticky
 .o-navFixedContainer
  .o-nav__content
   .a-brand
    a(href=linksUrl[1]): img.a-brand__Img(src=linksUrl[1]+logoResponsive[0], alt="Logotipo de monlogo")

   .a-burguer#burger
    img.a-burguer__menu(src=linksUrl[1]+"assets/img/icons/burguer.svg", alt="Icono de menu")

   .m-itemContent
    .a-burguerOut#burgerOut
     img.a-burguerOut__menu(src=linksUrl[1]+"assets/img/icons/close.svg", alt="Icono de cierre del menu")
    .a-brandMobile
     a(href=linksUrl[1]): img.a-brandMobile__Img(src=linksUrl[1]+logoResponsive[0], alt="Logo de monlogo")
    ul.m-itemContent__list
     li: a.a-itemLink(href=linksUrl[1]) Inicio
     li.m-dropdownMenu
      a.a-itemLink.m-dropdownMenu__item(href="#") #{titles[2]} #[i.-arrowDown.-hideMd]
      ul
       li: a.a-itemLink(href=linksUrl[1]+linksInternos[1]) #{services[0]}
       li: a.a-itemLink(href=linksUrl[1]+linksInternos[2]) #{services[1]}
       li: a.a-itemLink(href=linksUrl[1]+linksInternos[3]) #{services[2]}
     //- li.-hide.-showMd
      h3 Subscr√≠bete a nuestro newsletter
      +mailChimp("-footer", "", "-footer", "-btnFooter -btnMonlogo")

   .m-CallToAction
    a.a-button(href="mailto:"+linksExternos[1])
     button.a-btn.-btnPrimary(type="button") #{titles[4]}
    a.a-iconMail(href="mailto:"+linksExternos[1]): img.a-iconMail__img(src=linksUrl[1]+"assets/img/icons/mail.svg", alt="Icono para mandar email")