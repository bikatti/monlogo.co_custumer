head
 // Required meta tags
 meta(charset="UTF-8")
 meta(name="viewport", content="width=device-width, initial-scale=1.0, shrink-to-fit=no")
 meta(http-equiv="X-UA-Compatible", content="ie=edge")
 meta(name="og:type", content="Website")
 meta(name="og:site_name", content=titles[0])
 meta(name="og:locale", content="es_ES")
 meta(name="og:image", content="https://monlogo.co/assets/img/banner/banner.png")
 meta(name="og:image:alt", content="Logo de Monlogo con fondo azul rodeado de elementos de website")
 meta(name="og:image:type", content="image/png")
 meta(name="og:image:secure_url", content="https://monlogo.co/assets/img/banner/banner.png")
 meta(name="og:description", content="Llevamos tu negocio al mundo digital, en un mundo cada vez más tecnológico, hacemos la transformación digital de tu negocio aumentando su exposición a nivel mundial las 24 horas/todos los días del año.")
 meta(name="keywords", content="diseño,Monlogo,monlogo,Diseño,agencia de diseño,en Venezuela,Crea tu página web,página web,building your website,transformacion digital,Promotores,Hacer una transformacion digital")
 meta(name="apple-mobile-web-app-title", content="Monlogo")
 meta(name="format-detection", content="telephone=no")
 meta(name="author", content="Monlogo team")
 meta(name="theme-color", content="#226ef2")
 
 // Meta twitter
 meta(name="twitter:card", content="summary")
 meta(name="twitter:site", content="@monlogo")
 meta(name="twitter:title", content="Monlogo | Es hora de tener una transformación digital")
 meta(name="twitter:description", content="Llevamos tu negocio al mundo digital, en un mundo cada vez más tecnológico, hacemos la transformación digital de tu negocio aumentando su exposición a nivel mundial las 24 horas/todos los días del año.")
 meta(name="twitter:image", content="https://monlogo.co/assets/img/banner/banner.png")
 meta(name="twitter:image:alt", content="Logo de Monlogo con fondo azul rodeado de elementos de website")



 // metas de fav
 link(rel="shortcut icon", href=linksUrl[1]+logoResponsive[1], type="image/x-icon")
 link(rel="apple-touch-icon", href=linksUrl[1]+logoResponsive[1], type="image/png", sizes="180x180")
 link(rel="icon", href=linksUrl[1]+logoResponsive[1], type="image/png", sizes="196x196")
 

 // Bootstrap CSS
 link(rel="stylesheet", href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css", integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh", crossorigin="anonymous")
 link(rel="stylesheet", href=linksUrl[1]+"assets/css/style.css")
